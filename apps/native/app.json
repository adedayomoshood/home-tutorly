{
  "expo": {
    "name": "Home Tutorly",
    "slug": "home-tutorly",
    "scheme": "home-tutorly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/IBMPlexSans-Light.ttf",
            "./assets/fonts/WixMadeforDisplay-Regular.ttf",
            "./assets/fonts/WixMadeforDisplay-Medium.ttf",
            "./assets/fonts/WixMadeforDisplay-Bold.ttf",
            "./assets/fonts/WixMadeforDisplay-ExtraBold.ttf"
          ]
        }
      ]
    ]
  }
}
